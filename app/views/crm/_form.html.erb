
<%= simple_form_for @prospect, :url => {:action =>"update_prospect"} do |f| %>
  <%= f.input :date_prospect %>
  <%= f.input :source, collection:["Brochure site", "Tel","Fb Ads","IG Ads","Email","Autre"]%>
  <%= f.input :statut %>
  <%= f.input :lastname %>
  <%= f.input :firstname %>
  <%= f.input :email %>
  <%= f.input :tel %>
  <%= f.input :institut %>
  <%= f.input :cp %>
  <%= f.input :town %>
  <%= f.input :country %>
  <%= f.input :commercial, collection:["Emilie Vanhoutte","Marine Toussaint","Theania Frantz"] %>
  <%= f.input :comment %>
  <%= f.submit %>
<% end %>
